block vars
  - pageVars = null

doctype html
html(lang='ja')
  head
    block head
      meta(charset='UTF-8')
      meta(name='viewport', content='width=device-width, initial-scale=1.0')
      meta(http-equiv='X-UA-Compatible', content='ie=edge')
      link(rel='stylesheet', href=relativeRoot + 'assets/css/app.css')
      title=siteMeta.title
  body
    block header
      include _header
    block main

    block footer
      include _footer
